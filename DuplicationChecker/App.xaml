<Application x:Class="DuplicationChecker.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DuplicationChecker" xmlns:converters="clr-namespace:DuplicationChecker.Converters" xmlns:converters1="clr-namespace:FileDuplicationChecker.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:ColorToBrushConverter x:Key="ColorToBrushConverter"></converters:ColorToBrushConverter>
        <converters:ScientificNotationConverter x:Key="ScientificNotationToCommaSeperatedStringConverter"/>
        <converters:FileSizeFormater x:Key="HummanReadableFileSizeConverter"></converters:FileSizeFormater>
        <converters1:FormatDateTimeToStringConverter x:Key="DatetimeToStringConverter"/>
    </Application.Resources>
</Application>
